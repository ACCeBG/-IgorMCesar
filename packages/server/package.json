{
  "name": "@axiewatch/server",
  "version": "1.0.0",
  "main": "src/index.ts",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start:queue": "node -r ts-node/register/transpile-only -r tsconfig-paths/register ./dist/queue.js",
    "start:server": "node -r ts-node/register/transpile-only -r tsconfig-paths/register ./dist/server.js",
    "dev:queue": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register ./src/queue.ts",
    "dev:server": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register ./src/server.ts",
    "prod:db:migrate": "dotenv -e .env.production.local node-pg-migrate",
    "dev:db:migrate": "dotenv -e .env.development.local node-pg-migrate"
  },
  "devDependencies": {
    "@types/async": "^3.2.7",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/ioredis": "^4.27.7",
    "@types/node": "^16.4.13",
    "@types/node-cron": "^2.0.4",
    "dotenv-cli": "^4.0.0",
    "prisma": "^2.28.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@prisma/client": "^2.28.0",
    "@supabase/supabase-js": "^1.22.1",
    "axios": "^0.21.1",
    "bullmq": "^1.40.4",
    "cors": "^2.8.5",
    "dayjs": "^1.10.6",
    "dotenv": "^10.0.0",
    "ethers": "^5.4.4",
    "express": "^4.17.1",
    "ioredis": "^4.27.7",
    "knex": "^0.95.11",
    "node-cron": "^3.0.0",
    "node-pg-migrate": "^5.10.0",
    "objection": "^3.0.0-alpha.5",
    "pg": "^8.7.1"
  }
}
